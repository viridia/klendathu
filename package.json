{
  "name": "klendathu",
  "version": "1.0.0",
  "description": "Flexible bug-tracking and project management system.",
  "main": "index.js",
  "scripts": {
    "build-client": "parcel build client/src/index.html",
    "client": "node client/devServer",
    "gallery": "parcel client/src/gallery.html",
    "gen-icons": "cd client/src && npx @svgr/cli --ext tsx --out-dir=svg-compiled icons/*.svg",
    "gen-types": "gql-gen --config codegen.yml",
    "lint": "npm run lint-client && npm run lint-server",
    "lint-client": "eslint --ext ts,tsx ./client/src",
    "lint-server": "eslint --ext ts ./server/src ./server/tests",
    "manage": "ts-node bin/src/manage.ts",
    "start": "nodemon -w ./server/src --ext ts --exec ts-node -- -P ./server/tsconfig.json ./server/src/index.ts",
    "test-client": "jest --config client/jest.config.js",
    "test-server": "LOG_LEVEL=warn jest --runInBand --config server/jest.config.js",
    "test": "npm run test-server && npm run test-client"
  },
  "keywords": [
    "bug",
    "tracking"
  ],
  "author": "Talin <viridia@gmail.com>",
  "license": "Apache-2.0",
